# unsecured voice chat
netcat -vulp 12344 | opusdec --force-wav - - | aplay
arecord - | opusenc --bitrate 16 --ignorelength --expect-loss 10 --max-delay 10 --framesize 10 - - | netcat -u <dest_ip> 12344

